vertex_program StdQuad glsl glsles hlsl
{
	source stdquad.vert
}

vertex_program RayCast glsl glsles hlsl
{
	source raycast.vert
}

fragment_program Copy glsl glsles hlsl
{
	source copy.frag
}

fragment_program Copyback glsl glsles hlsl
{
	source copyback.frag

    default_params
    {
        param_named_auto ViewportSize viewport_size
    }
}

fragment_program Copy2x glsl glsles hlsl
{
	source copy2x.frag
}
