compositor Copy
{
    technique
    {
        texture_ref rt_ MRT rt
        texture_ref rt MRT rt

        target rt_
        {
            pass render_quad
            {
                material Copy
                input 0 rt
            }
        }

        target rt
        {
            pass render_quad
            {
                material Copy
                input 0 rt
            }
        }
    }
}
