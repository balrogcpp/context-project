//----------------------------------------------------------------------------------------------------------------------
// STD
//----------------------------------------------------------------------------------------------------------------------
abstract compositor STD
{
    technique
    {
        compositor_logic DeferredLogic
        texture_ref mrt MRT mrt
        texture rt target_width target_height PF_FLOAT16_RGB no_fsaa depth_pool 0 pooled

        target rt
        {
            input previous
        }
    }
}

abstract compositor Bypass
{
    technique
    {
        compositor_logic DeferredLogic
        texture_ref mrt MRT mrt

        target_output
        {
            input previous
        }
    }
}
