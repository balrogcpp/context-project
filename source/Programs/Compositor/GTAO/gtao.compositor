import STD from std.compositor

compositor GTAO : STD
{
    technique
    {
        texture_ref ao MRT ao
        texture_ref rt1 MRT rt1

        target_output
        {
            pass render_quad
            {
                identifier 10
                quad_normals camera_far_corners_view_space
                material GTAO
                input 0 mrt 1
                input 1 mrt 1
            }
        }
    }
}
