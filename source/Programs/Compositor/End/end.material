import STD from std.material

fragment_program End glsl glsles hlsl
{
    source end.frag
    target ps_2_0
}

material End : STD
{
	technique
	{
		pass
		{
            fragment_program_ref End
			{
                param_named RT int 0
			}

			texture_unit RT
            {
                texture black.dds
                tex_address_mode clamp
                filtering bilinear
            }
		}
	}
}
