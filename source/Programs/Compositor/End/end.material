import Base from base.material

//----------------------------------------------------------------------------------------------------------------------
//
//----------------------------------------------------------------------------------------------------------------------
fragment_program End glsl glsles hlsl
{
    source end.frag
}

material End : Base
{
	technique
	{
		pass
		{
            fragment_program_ref End
			{
                param_named RT int 0
			}

			texture_unit RT
            {
                texture black.dds
                tex_address_mode clamp
                filtering bilinear
            }
		}
	}
}
