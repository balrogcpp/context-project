vertex_program StdQuad glsl glsles hlsl
{
	source stdquad.vert
    target vs_2_0

	default_params
	{
		param_named_auto WorldViewProj worldviewproj_matrix
	}
}

vertex_program RayCast glsl glsles hlsl
{
	source raycast.vert
    target vs_2_0

	default_params
	{
		param_named_auto WorldViewProj worldviewproj_matrix
	}
}
